<h1>Meet the dudes</h1>
<div class="tx-ttaddress-pi1 people clearfix" id="team-cards">
  <div class="items">
  <%- @team.each_slice(5) do |slice| -%>
    <div class="team-slice">
    <%- slice.each do |data| -%>
    <div class="vcard panel">
      <img src="<%= data[:picture] %>" width="167" height="167" alt="<%= data[:name] %>" title="<%= data[:name] %>" />
      <h2><%= data[:name] %></h2>
      <h3><%= data[:position] %></h3>
      <p><%= data[:description] %></p>
      <div class="adr"></div>
    </div>
    <%- end -%>
    </div>
  <%- end -%>
  </div>
  <div class="cb"></div>
</div>
<a class="prev browse left">back</a>
<a class="next browse right">forward</a>
<script type="text/javascript">
  jQuery("#team-cards").scrollable();
</script>

